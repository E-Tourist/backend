{
  "name": "backend",
  "version": "1.0.0",
  "description": "This is backend E-Tourist service written in NodeJS using Typescript",
  "main": "dist/src/server.js",
  "scripts": {
    "server": "npm run migrate && node -r ./tsconfig-paths-bootstrap.js dist/src/server.js",
    "build": "npm run clean && npx tsc && tsc-alias && npm run tslint && npm run redoc:bundle && npm run copy-static",
    "watch-ts": "npx tsc -w",
    "watch-node": "nodemon -r ./tsconfig-paths-bootstrap.js dist/src/server.js | npx pino-pretty -c -t",
    "watch": "npm run migrate && npx concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:verb": "jest --verbose",
    "tslint": "npx tslint -c tslint.json -p tsconfig.json",
    "clean": "rimraf ./dist",
    "copy-static": "mkdirp ./dist/views && copy /Y .\\views\\* .\\dist\\views",
    "generate:swagger": "node -r ./tsconfig-paths-bootstrap.js dist/src/generate-swagger.js && npx @redocly/cli build-docs ./dist/public/swagger.json -o views/api.html",
    "redoc:bundle": "npm run generate:swagger && npx @redocly/cli build-docs ./dist/public/swagger.json -o views/api.html",
    "migrate": "node -r dotenv/config -r ./tsconfig-paths-bootstrap.js dist/src/migrations/index.js up",
    "migrate:down": "node -r ./tsconfig-paths-bootstrap.js dist/src/migrations/index.js down",
    "migrate:new": "npx mongo-migrate new"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:E-Tourist/backend.git"
  },
  "keywords": [
    "api",
    "typescript"
  ],
  "author": "E-Tourist",
  "license": "MIT",
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/crypto-js": "^4.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.17",
    "@types/express-pino-logger": "^4.0.3",
    "@types/express-rate-limit": "^6.0.0",
    "@types/express-session": "^1.17.4",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.181",
    "@types/method-override": "^0.0.32",
    "@types/mime": "^2.0.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.23",
    "@types/node-schedule": "^1.3.2",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/pino": "^7.0.5",
    "@types/redis": "^4.0.11",
    "@types/stoppable": "^1.1.1",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-dist": "^3.30.1",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.1.0",
    "copyfiles": "^2.4.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^7.6.0",
    "redoc-cli": "^0.13.21",
    "rimraf": "^5.0.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@google/maps": "^1.1.3",
    "@googlemaps/google-maps-services-js": "^3.3.31",
    "bluebird": "^3.7.2",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.1.4",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-pino-logger": "^7.0.0",
    "express-rate-limit": "^6.3.0",
    "express-session": "^1.17.2",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "immutable": "^4.1.0",
    "jwt-simple": "^0.5.6",
    "keycloak-connect": "^19.0.3",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "mime": "^3.0.0",
    "mkdirp": "^3.0.1",
    "moment": "^2.29.2",
    "mongo-migrate-ts": "^1.1.1",
    "mongodb": "4.9.1",
    "mongoose": "6.6.1",
    "mongoose-paginate-v2": "^1.7.0",
    "multer": "^1.4.5-lts.1",
    "node-schedule": "^2.1.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "pino": "^7.11.0",
    "pug": "^3.0.2",
    "sha3": "^2.1.4",
    "stoppable": "^1.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^4.13.0",
    "swagger-ui-express": "^4.6.3"
  },
  "engines": {
    "node": ">=17.8.0"
  }
}
